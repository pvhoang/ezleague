  "scripts": {
    "ng": "ng",
    "start": "npm install --legacy-peer-deps && node --max_old_space_size=8192 ./node_modules/@angular/cli/bin/ng serve --open",
    "start-proxy": "ng serve --open --proxy-config proxy.conf.json",
    "start-hmr": "ng serve --configuration hmr --source-map=false --hmr-warning=false",
    "start-hmr-sourcemaps": "ng serve --configuration hmr --source-map=true --hmr-warning=false",
    "build": "node --max_old_space_size=8192 ./node_modules/@angular/cli/bin/ng build --configuration=production",
    "build-stats": "node --max_old_space_size=8192 ./node_modules/@angular/cli/bin/ng build --stats-json",
    "build:prod": "ng build --configuration=production",
    "build-android": "ng build && npx cap sync android && npx cap run android",
    "build:prod-android": "ng build --configuration=production && npx cap sync android && npx cap open android",
    "build:prod-stats": "node --max_old_space_size=8192 ./node_modules/@angular/cli/bin/ng build --configuration production --stats-json",
    "bundle-analyzer": "webpack-bundle-analyzer dist/vuexy/stats.json",
    "ckeditor:install": "npm install ./ckeditor5 --legacy-peer-deps",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "ionic:build": "npm run build:prod",
    "ionic:serve": "npm run start"
  },
